# CLUSTAL language definition file for less pipe
# bioSyntax v0.1
# Depends on fasta.style + bioSyntax.outlang

# Start of file CLUSTAL Header
header start '^CLUSTAL'

# Data Name
comment = '^\S++(?=\s)'

ntA = '[Aa]+'
ntT = '[Tt]+'
ntG = '[Gg]+'
ntC = '[Cc]+'
ntN = '[NnXx]+'
ntT = '[Uu]+'

# No Highlight --
# ntGap = '[.-]+' #Gap sequence
# numeric = '[0-9]+$' # Trailing Number
