# Flagstat (.flagstat) language definition file for less pipe
# bioSyntax v0.1
# Depends on sam.style + bioSyntax.outlang

# QC-Passed Reads
numeric = 'QC-passed reads'
numeric = '^\d+'
numeric = '\([\d\.]+%'
numeric = '\(\-?nan%'

#QC-Failed Reads
keyword = 'QC-failed reads'
keyword = '(?<=\+ )\d+'
keyword = '\:[\d\.]+%'
keyword = '\:\-?nan%'

#string = 'mapQ'
